{
    "name": "renamed-to/renamed-php",
    "description": "Official PHP SDK for the renamed.to API - AI-powered file renaming, PDF splitting, and data extraction",
    "type": "library",
    "license": "MIT",
    "keywords": ["renamed", "ai", "document", "rename", "pdf", "extract", "ocr", "file-processing"],
    "homepage": "https://www.renamed.to",
    "authors": [
        {
            "name": "Renamed.to",
            "email": "support@renamed.to"
        }
    ],
    "support": {
        "issues": "https://github.com/renamed-to/renamed-sdk/issues",
        "source": "https://github.com/renamed-to/renamed-sdk",
        "docs": "https://www.renamed.to/docs"
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Renamed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Renamed\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
