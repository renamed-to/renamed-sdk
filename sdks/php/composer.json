{
    "name": "renamed/sdk",
    "description": "Official PHP SDK for the renamed.to API",
    "version": "0.1.0",
    "type": "library",
    "license": "MIT",
    "keywords": ["renamed", "ai", "document", "rename", "pdf", "extract"],
    "homepage": "https://renamed.to",
    "authors": [
        {
            "name": "Renamed.to",
            "email": "support@renamed.to"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Renamed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Renamed\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
